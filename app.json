{
	"name": "appwrite-lite",
	"description": "Appwrite Lite",
	"repository": "https://github.com/appwrite/lite",
	"logo": "https://appwrite.io/images/appwrite.svg",
	"keywords": [ "appwrite", "docker", "php" ],
	"env": {
		"_APP_ENV": { "value": "production" },
		"_APP_SYSTEM_EMAIL_NAME": { "value": "Appwritelite" },
		"_APP_SYSTEM_EMAIL_ADDRESS": { "value": "team@appwrite.io" },
		"_APP_SYSTEM_SECURITY_EMAIL_ADDRESS": { "value": "security@appwrite.io" },
		"_APP_OPTIONS_ABUSE": { "value": "disabled" },
		"_APP_OPTIONS_FORCE_HTTPS": { "value": "disabled" },
		"_APP_OPENSSL_KEY_V1": { "value": "your-secret-key" },
		"_APP_DOMAIN": { "value": "demo.appwrite.io" },
		"_APP_DOMAIN_TARGET": { "value": "demo.appwrite.io" },
		"_APP_STORAGE_LIMIT": { "value": "10000000" },
		"_APP_FUNCTIONS_TIMEOUT": { "value": "900" },
		"_APP_FUNCTIONS_CONTAINERS": { "value": "10" },
		"_APP_FUNCTIONS_CPUS": { "value": "1" },
		"_APP_FUNCTIONS_MEMORY": { "value": "128" },
		"_APP_FUNCTIONS_MEMORY_SWAP": { "value": "128" },
		"_APP_MAINTENANCE_INTERVAL": { "value": "86400" }
	},
	"addons": [
		{
			"plan":"jawsdb-maria:kitefin"
		},
		{
			"plan":"heroku-redis"
		}
	],
	"stack": "container"
}
